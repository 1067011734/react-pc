{
  "compilerOptions": {
    "target": "esnext",
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
    },
    "allowSyntheticDefaultImports": true,
    "noImplicitAny":false
  }
}
